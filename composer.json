{
  "name": "oro/upgrade-toolkit",
  "description": "The Oro Source Code Upgrade Toolkit",
  "homepage": "https://github.com/oroinc/upgrade",
  "keywords": [
    "OroPlatform",
    "OroCommerce",
    "OroCRM",
    "upgrade",
    "update"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Oro, Inc",
      "homepage": "https://oroinc.com"
    }
  ],
  "require": {
    "rector/rector": "^0.16.0",
    "oro/platform": "~6.0.0"
  },
  "autoload": {
    "psr-4": {
      "Oro\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Oro\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.2"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    }
  }
}
