{
  "name": "oro/upgrade-toolkit",
  "description": "The Oro Source Code Upgrade Toolkit",
  "homepage": "https://github.com/oroinc/upgrade",
  "license": "MIT",
  "authors": [
    {
      "name": "Oro, Inc",
      "homepage": "https://oroinc.com/"
    }
  ],
  "keywords": [
    "OroPlatform",
    "OroCommerce",
    "OroCRM",
    "upgrade",
    "update"
  ],
  "bin": [
    "bin/upgrade-toolkit"
  ],
  "require": {
    "phpstan/phpstan": "2.1.38",
    "rector/rector": "2.3.6",
    "symfony/maker-bundle": "^v1.52.0",
    "nette/utils": "^3.2.9 || ^4.0"
  },
  "autoload": {
    "psr-4": {
      "Oro\\UpgradeToolkit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Oro\\UpgradeToolkit\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~9.5.27 || ^10.2"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    }
  }
}
