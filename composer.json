{
  "name": "oro/upgrade-toolkit",
  "require": {
    "rector/rector": "^0.16.0",
    "oro/platform": "~5.1.0"
  },
  "autoload": {
    "psr-4": {
      "Oro\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Oro\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.2"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    }
  }
}
